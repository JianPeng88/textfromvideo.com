# 设置所有CSS文件的正确MIME类型
/css/*
  Content-Type: text/css

# 设置所有JavaScript文件的正确MIME类型
/js/*
  Content-Type: application/javascript

# 设置单个文件
/main.js
  Content-Type: application/javascript

/style.css
  Content-Type: text/css

/responsive.css
  Content-Type: text/css

/animations.css
  Content-Type: text/css

/all.min.css
  Content-Type: text/css

# 设置字体文件的MIME类型
/*.woff2
  Content-Type: font/woff2
